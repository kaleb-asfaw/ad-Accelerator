{
  "manifest_version": 3,
  "name": "Ad Acceleration",
  "version": "1.0.4", 
  "description": "YouTube premium on a budget :)",
  "permissions": ["storage"], 
  "background":{
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "action":{
    "default_popup": "popup.html"
  },
  "icons":{
    "128": "images/128.png"
  }
}
