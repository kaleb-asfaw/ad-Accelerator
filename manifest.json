{
  "manifest_version": 3,
  "name": "Auto Skip Ads",
  "version": "3.0.0", 
  "description": "Extension which skips both 5-second skippable and unskippable ads",
  "permissions": ["tabs", "storage"], 
  "background":{
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "action":{
    "default_popup": "popup.html"
  }
}